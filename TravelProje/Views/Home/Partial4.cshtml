@using TravelProje.Models.Class
@model List<Blog>

<div class="padding1">
    <h4>En beğendiğim yerler</h4>
    <div class="box-left">
        @foreach (var m in Model)
        {
        <div class="bot-img">
            <img src="@m.BlogImage" alt="" />
        </div>
        <div class="bot-desc">
            <h1>@m.Baslik</h1>
            <p>@m.Aciklama.Substring(0,m.Aciklama.Substring(0,85).LastIndexOf(" "))</p>
        </div>
        <div class="clear"></div>
        }
    </div>
        @Html.Action("Partial5","Home")
</div>
