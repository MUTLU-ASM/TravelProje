@using TravelProje.Models.Class
@model List<Blog>
<div class="padding">
    <h4>Bu haftanın ilk 5'i</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var m in Model)
        {
        <li class="list-bg"><span>@{i++;} @i.</span><a href="/Blog/BlogDetay/@m.Id">@m.Baslik</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Tüm Blogları Gör</a>
</div>
